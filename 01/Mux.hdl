/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    And(a=sel, b=b, out=selAndB);
    Not(in=sel, out=notSel);
    And(a=a, b=notSel, out=notSelAndA);
    Xor(a=selAndB, b=notSelAndA, out=out);
}